#!/usr/bin/env tclsh

package require tcltest
namespace import tcltest::*

#software under test
package require gen

#proc range
test range_1_1 {
    Test: range 1 1 - should be empty ""
} -body {
    gen::range 1 1
} -result ""

test range_1_2 {
    Test: range 1 2 - should produce an output list of "1"
} -body {
    gen::range 1 2
} -result 1

test range_1_10 {
    Test: range 1 10 - should produce list: 1..9
} -body {
    gen::range 1 10
} -result [list 1 2 3 4 5 6 7 8 9]

test range_2_10_2 {
    test: range 2 10 2 - should produce list: 2 4 6 8
} -body {
    gen::range 2 10 2
} -result [list 2 4 6 8]

#proc ipv4
test ipv4_count_0 {
    test: ipv4 with count of 0
} -body {
    gen::ipv4 "10.0.0.1" "0"
} -result ""

test ipv4_count_1 {
    test: ipv4 with count of 1
} -body {
    gen::ipv4 "10.0.0.1" "1"
} -result "10.0.0.1"

test test_ipv4_1024_incr_third_octet {
    test: test_ipv4_1024_incr_third_octet
} -body {
    lindex [gen::ipv4 "192.0.0.0" "1024" "0.0.1.0"] end
} -result "192.3.255.0"



cleanupTests
